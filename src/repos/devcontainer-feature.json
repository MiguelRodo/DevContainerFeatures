{
    "name": "Automatically set up multi-repo projects",
    "id": "repos",
    "version": "1.8.0",
    "description": "Add commands to work with multiple repos",
    "postCreateCommand": "[ \"$CODESPACES\" = \"true\" ] && [ -n \"$GH_TOKEN\" ] && /usr/local/bin/repos-post-create || true",
    "postStartCommand": "/usr/local/bin/repos-post-start"
}
